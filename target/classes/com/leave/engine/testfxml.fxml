         <children>
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="220.0" layoutY="112.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../../../../../Downloads/aiden-pedroncelli-playerroom.jpg" />
